<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />
        <service id="maker.maker.make_module" class="Dev\Maker\Maker\MakeModule">
            <argument type="service" id="maker.file_manager" />
            <argument type="service" id="maker.doctrine_helper" />
            <argument>null</argument>
            <argument type="service" id="maker.custom_generator" />
            <argument type="service" id="maker.module_entity_class_generator" />
            <argument type="service" id="maker.php_compat_util" />
            <tag name="maker.command" />
        </service>
    </services>
</container>
